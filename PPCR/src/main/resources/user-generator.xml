<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<context id="user">
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/ppcr" userId="root"
			password="password" />
		<javaModelGenerator targetPackage="com.nono.ppcr.user.dao.model"
			targetProject="PPCR" />
		<sqlMapGenerator targetPackage="com.nono.ppcr.user.dao.mapper"
			targetProject="PPCR"></sqlMapGenerator>
		<javaClientGenerator targetPackage="com.nono.ppcr.user.dao.mapper"
			targetProject="PPCR" type="XMLMAPPER" />
		<table schema="" tableName="user"></table>
	</context>
</generatorConfiguration>  